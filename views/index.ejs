
<h3>Halo <%= nama  %> </h3>
<p>selamat datang di situs saya</p>

<h4>Daftar murid</h4> 

<!-- cek jika data kosong  -->
<% if (murid.length == 0) { %>
    <p>data murid masih kosong</p>
<% } else { %>
    looping data murid
    <% murid.forEach(mrd => { %>
        <ul>
            <li><%= mrd.nama %> </li>
            <li><%= mrd.email %> </li>
        </ul>
    <% }) %>
<% } %>

